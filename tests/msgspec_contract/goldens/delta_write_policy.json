{
  "enable_features": [
    "change_data_feed"
  ],
  "parquet_writer_policy": {
    "bloom_filter_enabled": [
      "node_id"
    ],
    "bloom_filter_fpp": 0.01,
    "bloom_filter_ndv": 1000,
    "dictionary_enabled": [
      "node_kind"
    ],
    "statistics_enabled": [
      "node_id",
      "path"
    ]
  },
  "partition_by": [
    "repo"
  ],
  "stats_columns": [
    "node_id",
    "path"
  ],
  "stats_max_columns": 16,
  "stats_policy": "explicit",
  "target_file_size": 128,
  "zorder_by": [
    "node_id"
  ]
}
