{
  "typeCheckingMode": "basic",
  "pythonVersion": "3.13",
  "pythonPlatform": "Linux",
  "venvPath": ".",
  "venv": ".venv",
  "useLibraryCodeForTypes": true,
  "include": ["src", "tests", "tools"],
  "exclude": [
    ".git",
    ".venv",
    ".ruff_cache",
    ".pytest_cache",
    "docs/**",
    "rust/**"
  ],
  "extraPaths": [
    ".",
    "src",
    "tests",
    "tools"
  ],
  "executionEnvironments": [
    {
      "root": "src",
      "extraPaths": [
        ".",
        "src",
        "tests",
        "tools"
      ],
      "pythonVersion": "3.13"
    },
    {
      "root": "tests",
      "extraPaths": [
        ".",
        "src",
        "tests",
        "tools"
      ],
      "pythonVersion": "3.13"
    }
    
  ]
}
